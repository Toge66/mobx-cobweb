{
  "name": "@whispered/skeleton-orm",
  "version": "0.0.1",
  "description": "ORM for Frontend",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf dist && npm run optimize && npm run copy",
    "optimize": "npm run build:lib && npm run build:esm && npm run build:types",
    "copy": "cp package.json README.md .npmignore dist",
    "publish2npm": "npm publish dist",
    "build:lib": "tsc --rootDir src --outDir dist/lib --module commonjs",
    "build:esm": "tsc --rootDir src --outDir dist/esm --module esnext",
    "build:types": "tsc --rootDir src --outDir dist/types --emitDeclarationOnly --declaration",
    "test": "jest"
  },
  "peerDependencies": {
    "tslib": "^2"
  },
  "resolutions": {
    "fsevents": ""
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^25.2.3",
    "glob": "^7.1.6",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "nock": "^12.0.3",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "datx": "^1.0.1",
    "datx-utils": "^1.0.0",
    "mobx": "^4 || ^5",
    "tslib": "^2",
    "reflect-metadata": "latest"
  }
}
